{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    },
    "logLevel": {
      "Function": "Information",
      "Host.Results": "Trace",
      "Host.Aggregator": "Warning",
      "Function.FutureProspect": "Information",
      "FunctionWithTraceLog.Bl": "Trace",
      "Default": "Warning"
    }
  }
}