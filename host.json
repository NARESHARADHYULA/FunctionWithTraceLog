{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    },
    "logLevel": {
      "Azure.Core": "Error",
      "Azure.Identity": "Critical",
      "Function": "Information",
      "Host.Results": "Trace",
      "Host.Aggregator": "Warning",
      "Function.FutureProspect": "Trace",
      "Default": "Warning"
    }
  }
}